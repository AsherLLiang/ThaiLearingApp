{
  "name": "vocabulary",
  "version": "1.2.0",
  "description": "Thai Learning App - 词汇学习云函数 (模块化版本)",
  "main": "index.js",
  "scripts": {
    "deploy": "tcb fn deploy vocabulary",
    "logs": "tcb fn log vocabulary",
    "test": "tcb fn invoke vocabulary --data '{\"action\":\"getVocabularyList\",\"data\":{\"limit\":3}}'",
    "test:today": "tcb fn invoke vocabulary --data '{\"action\":\"getTodayWords\",\"data\":{\"userId\":\"test\",\"limit\":5}}'",
    "test:stats": "tcb fn invoke vocabulary --data '{\"action\":\"getReviewStatistics\",\"data\":{\"userId\":\"test\"}}'"
  },
  "dependencies": {
    "@cloudbase/node-sdk": "^2.4.9"
  },
  "author": "Thai Learning App Team",
  "license": "MIT",
  "keywords": [
    "thai",
    "language-learning",
    "vocabulary",
    "cloudbase",
    "sm2-algorithm",
    "spaced-repetition",
    "modular"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
